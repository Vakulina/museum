    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "mapbox-gl": "^2.15.0",
    "swiper": "^10.0.4",
